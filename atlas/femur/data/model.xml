<?xml version="1.0"?>
<model>
    <model-type>DeterministicAtlas</model-type>
    <dimension>3</dimension>
    <template>
        <object id="img">
            <deformable-object-type>Image</deformable-object-type>
            <noise-std>0.1</noise-std>
            <filename>mean_template.nii.gz</filename>
        </object>
    </template>
    <deformation-parameters>
        <kernel-width>2</kernel-width>
        <kernel-type>keops</kernel-type>
    </deformation-parameters>
 </model>
